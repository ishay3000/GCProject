<Window x:Class="GCProject.Views.DataTemplateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GCProject.Views"
        xmlns:vm="clr-namespace:GCProject.ViewModels"
		xmlns:views="clr-namespace:GCProject.Views"
        mc:Ignorable="d"
        Title="DataTemplateWindow" Height="300" Width="300">

	<Window.DataContext>
		<vm:MenuCardViewModel x:Name="CardViewModel"/>
	</Window.DataContext>
	<Grid>
		<ContentControl Content="{Binding}"></ContentControl>
    </Grid>
</Window>
